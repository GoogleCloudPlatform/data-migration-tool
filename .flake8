# Copied from https://github.com/googleapis/synthtool/blob/master/synthtool/gcp/templates/python_library/.flake8
[flake8]
ignore = E203, E266, E501, W503
exclude =
  # Exclude generated code.
  **/proto/**
  **/gapic/**
  *_pb2.py

  # Standard linting exemptions.
  __pycache__
  .git
  *.pyc
  conf.py
  venv

  # Nox staging
  .nox

  # pytest cache
  .pytest_cache
