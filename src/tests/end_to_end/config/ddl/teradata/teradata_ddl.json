{
    "batchDistribution": 1,
    "unique_id": "{{ unique_id }}",
    "type": "ddl",
    "source": "teradata",
    "extract_ddl": false,
    "translationErrorFilterRules": [
        {
            "field": "Category",
            "matchType": "equals",
            "value": "NoTargetSupportForFeature"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "NoTargetSupportForSetTables"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "NoTargetSupportForIndexBackedConstraints"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "NoTargetSupportForPartitionSemantics"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "FileRead"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "EmitterSuccess"
        },
        {
            "field": "Category",
            "matchType": "equals",
            "value": "TranspilerOutputQuality"
        }
    ],
    "migrationTask": {
        "type": "Translation_Teradata2BQ",
        "translationConfigDetails": {
            "sourceDialect": {
                "teradataDialect": {
                    "mode": "SQL"
                }
            },
            "sourceEnv": {
                "defaultDatabase": "{{ project_id }}"
            },
            "targetDialect": {
                "bigqueryDialect": {}
            },
            "gcsSourcePath": "gs://{{ bucket_name }}/input/ddl/teradata",
            "gcsTargetPath": "gs://{{ bucket_name }}/output/ddl/teradata",
            "nameMappingList": {
                "name_map": [
                    {
                        "source": {
                            "type": "SCHEMA",
                            "database": "{{ project_id }}",
                            "schema": "{{ source_schema }}"
                        },
                        "target": {
                            "database": "{{ project_id }}",
                            "schema": "{{ target_schema }}"
                        }
                    }
                ]
            }
        }
    },
    "validation_config": {
        "source_config": {
            "source_type": "Teradata",
            "host": "{{ source_ip }}",
            "port": 1025,
            "user-name": "{{ source_username }}",
            "password": "secret-{{ secret_name }}"
        },
        "target_config": {
            "target_type": "BigQuery",
            "project-id": "{{ project_id }}"
        },
        "validation_params_file_path": "gs://{{ config_bucket_name }}/validation/teradata/{{ validation_mapping_file }}",
        "validation_mode": "{{ validation_mode }}",
        "validation_type": "schema",
        "pod_operator_mem": "4000M",
        "pod_operator_cpu": "800m"
    }
}